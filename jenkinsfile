// This is to build maven and identify it's version

pipeline {
    agent any
    tools {
        Maven 'MAVEN_PATH'
    }
    stages {
        stage {
            steps {
                echo "This is to view maven version"
                sh 'mvn --version'
            }
        }
        stage ('specific stage')
        tools {
            jdk 'jdk-17'
            steps {
                echo "This is to specfie jdk version"
                sh 'java --version'
            }
        }
    }
}
